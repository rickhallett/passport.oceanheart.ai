{
  "name": "passport-elysia",
  "version": "1.0.0",
  "description": "Passport authentication service built with Elysia and Bun",
  "main": "src/server.ts",
  "module": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --hot src/server.ts",
    "start": "bun src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target bun",
    "migrate": "bun scripts/migrate.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "bun --bun tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/html": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/static": "^1.4.0",
    "elysia": "^1.4.5",
    "postgres": "^3.4.7",
    "zod": "^4.1.9"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "keywords": [
    "authentication",
    "elysia",
    "bun",
    "typescript",
    "passport",
    "oceanheart"
  ]
}
